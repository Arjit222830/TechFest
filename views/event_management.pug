<!DOCTTYPE html>
<html>
    include header.pug
    
    <body>

        -var head= ['S.No','Name','College','Year','Email','Contact','Date'];

        div(class="color")
            div(class="container")              
                div(class="pad")
                    h3(class="text-center content1")= "Registration Transaction Details"
                br

                div(style="overflow-x:auto")
                    table(class="text-center content table table-dark table-striped table-bordered")
                        col(width="10" height="1vh")
                        col(width="130")
                        col(width="130")
                        col(width="5")
                        col(width="130")
                        col(width="130")
                        col(width="130")
                        thead(class="heading thead-light")
                            tr
                                for r in head
                                    th= r
                               
                            tbody(class="eventable")
                                -for(var i=0;i<registers.length;i++)
                                    tr
                                        td= i+1
                                        td= registers[i].name
                                        td= registers[i].college_name
                                        td= registers[i].year
                                        td= registers[i].email
                                        td= registers[i].contact
                                        td= registers[i].date.toString().substring(0,(registers[i].date.toString().indexOf(':')-2))
                                        
            
    <script src="../vikiran.js"></script>

    </body>
</html>