<!DOCTTYPE html>
<html>
    include header.pug
    
    <body>

        -var head= ['S. No','Team Name','Team Leader','Enrollment No.','College Name','Tot. Mem','Contact',"Other Members",'Status','Date'];

        div(class="color")
            div(class="container")              
                div(class="pad")
                    h3(class="text-center content1")= event
                br

                div(style="overflow-x:auto")
                    table(class="text-center content table table-dark table-striped table-bordered")
                        col(width="10" height="1vh")
                        col(width="160")
                        col(width="160")
                        col(width="160")
                        col(width="160")
                        col(width="10")
                        col(width="160")
                        col(width="160")
                        col(width="10")
                        col(width="160")
                        thead(class="heading thead-light")
                            tr
                                -for(var i=0;i<head.length;i++)
                                    th= head[i]

                            tbody(class="eventable")
                                -r= Object.assign({}, registers)
                                -for(var i=0;i<registers.length;i++)
                                    -var count=0;
                                    each t in transactions
                                        -if(t.Transaction_No== r[i].transaction)
                                            -count++
                                            -break
                                    tr
                                        td= i+1
                                        td= r[i].team_name
                                        td= r[i].team_leader
                                        td= r[i].enroll_no
                                        td= r[i].college_name
                                        td= r[i].total_members
                                        td= r[i].contact
                                        -if(r[i].Other_Members!= undefined)
                                            -if(r[i].Other_Members['2']== undefined)
                                                td= r[i].Other_Members['1']
                                            -else if(r[i].Other_Members['3']== undefined)
                                                td= r[i].Other_Members['1']+","+r[i].Other_Members['2']
                                            -else if(r[i].Other_Members['4']== undefined)
                                                td= r[i].Other_Members['1']+","+r[i].Other_Members['2']+","+r[i].Other_Members['3']
                                            -else
                                                td= r[i].Other_Members['1']+","+r[i].Other_Members['2']+","+r[i].Other_Members['3']+","+r[i].Other_Members['4']
                                        -if(count!=0)
                                            td="Verified"
                                        -else
                                            td="-"
                                        td= r[i].date.toString().substring(0,(r[i].date.toString().indexOf(':')-2))
            
    <script src="../vikiran.js"></script>

    </body>
</html>